{
    "name": "slim/slim-skeleton",
    "description": "Restful API",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Teymur Mardaliyev",
            "email": "info@bul.com",
            "homepage": "http://www.bul.az/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "doctrine/orm": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "./src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
